    "homepage": "https://github.com/SerhiiCho/ago",
    "keywords": ["time ago", "library", "html"],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Serhii Cho",
            "email": "serhiicho@protonmail.com",
            "homepage": "https://serhii.tk",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Serhii\\Ago\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.1"
    },
